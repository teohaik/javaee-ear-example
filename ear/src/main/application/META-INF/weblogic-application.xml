<?xml version="1.0" encoding="UTF-8"?>
<weblogic-application xmlns="http://xmlns.oracle.com/weblogic/weblogic-application" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.oracle.com/weblogic/weblogic-application http://xmlns.oracle.com/weblogic/weblogic-application/1.7/weblogic-application.xsd">

  <library-ref>
    <library-name>CommonLibsWarForEar</library-name>
    <specification-version>1.0</specification-version>
    <implementation-version>1.0</implementation-version>
    <exact-match>false</exact-match>
  </library-ref>

  <session-descriptor>
    <persistent-store-type>memory</persistent-store-type>
    <sharing-enabled>true</sharing-enabled>
  </session-descriptor>


  <prefer-application-packages>
    <package-name>org.slf4j.*</package-name>
    <package-name>log4j.*</package-name>
  </prefer-application-packages>


</weblogic-application>
